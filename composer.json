{
    "name": "matware-lab/webservices",
    "type": "joomla-package",
    "description": "Joomla Webservices Application",
    "keywords": ["joomla", "cms", "webservices"],
    "homepage": "https://github.com/joomla-projects/webservices",
    "license": "GPL-2.0+",
    "minimum-stability" : "dev",
    "config": {
       "preferred-install": "dist",
       "platform": {
           "php": "5.5.9"
       }
    },
    "require": {
        "php": ">=5.5.9",
        "joomla/application": "~1.5",
        "joomla/database": "~1.2",
        "joomla/input": "~1.2",
        "joomla/filter": "~1.1",
        "joomla/event": "~1.1",
        "joomla/uri": "~1.1",
        "joomla/filesystem": "~1.2",
        "joomla/authentication": "^1.1.1",
        "joomla/registry": "~1.4",
        "joomla/di": "~1.2",
        "joomla/utilities": "~1.3 >=1.3.3",
        "joomla/date": "2.0.x-dev",
        "joomla/language": "~1.0",
        "joomla/session": "~1.0",
        "psr/log": "~1.0",
        "willdurand/Negotiation": "^2.0",
        "joomla/router": "dev-2.0-dev"
    },
    "require-dev": {
        "codeception/codeception": "~2.1",
        "joomla-projects/joomla-browser": "dev-develop",
        "codegyre/robo": "dev-master",
        "flow/jsonpath": "dev-master",
        "phing/phing": "~2",
        "joomla-projects/robo": "dev-master",
        "joomla-projects/selenium-server-standalone": "v2.47.1",
        "cloudinary/cloudinary_php": "1.1.1"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Webservices\\": "src/"
        }
    }
}
